tail#_ {bn:#} b:(bits bn) = SnakeData ~0; cons#_ {bn:#} {n:#} b:(bits bn) next:^(SnakeData ~n) = SnakeData ~(n + 1);  chunked_data#_ data:(HashMapE 32 ^(SnakeData ~0)) = ChunkedData;  text#_ {n:#} data:(SnakeData ~n) = Text; snake#00 data:(SnakeData ~n) = ContentData; chunks#01 data:ChunkedData = ContentData; onchain#00 data:(HashMapE 256 ^ContentData) = FullContent; offchain#01 uri:Text = FullContent;
